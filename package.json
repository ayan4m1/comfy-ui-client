{
  "name": "@ayan4m1/comfy-ui-client",
  "version": "0.4.0",
  "engines": {
    "node": ">=22"
  },
  "description": "WebSockets API client for ComfyUI",
  "author": "ayan4m1 <andrew@bulletlogic.com>",
  "license": "MIT",
  "homepage": "https://github.com/ayan4m1/comfy-ui-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayan4m1/comfy-ui-client.git"
  },
  "bugs": {
    "url": "https://github.com/ayan4m1/comfy-ui-client/issues"
  },
  "keywords": [
    "comfy-ui",
    "comfy-ui node",
    "comfy-ui node API",
    "comfy-ui API",
    "comfy-ui sdk",
    "comfy-ui-node",
    "comfy-ui-node-api",
    "comfy-ui-client",
    "comfy-ui-sdk"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm,iife --platform browser --globalName ComfyUIClient",
    "dev": "npm run build --watch",
    "lint": "eslint .",
    "prettier": "prettier --config .prettierrc --write ./src",
    "release": "npm run build && npm publish --access public"
  },
  "dependencies": {
    "globby": "^14.1.0",
    "isomorphic-ws": "^5.0.0",
    "pino": "^8.14.2",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.8",
    "@types/node": "^18.11.14",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "tsup": "^7.1.0",
    "typescript": "~5.0"
  },
  "directories": {
    "example": "examples"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184"
}
