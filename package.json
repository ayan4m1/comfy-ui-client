{
  "name": "@ayan4m1/comfy-ui-client",
  "version": "0.4.3",
  "engines": {
    "node": ">=22"
  },
  "description": "WebSockets API client for ComfyUI",
  "author": "ayan4m1 <andrew@bulletlogic.com>",
  "license": "MIT",
  "homepage": "https://github.com/ayan4m1/comfy-ui-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayan4m1/comfy-ui-client.git"
  },
  "bugs": {
    "url": "https://github.com/ayan4m1/comfy-ui-client/issues"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "keywords": [
    "comfy-ui",
    "comfy-ui node",
    "comfy-ui node API",
    "comfy-ui API",
    "comfy-ui sdk",
    "comfy-ui-node",
    "comfy-ui-node-api",
    "comfy-ui-client",
    "comfy-ui-sdk"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm,iife --platform browser --globalName ComfyUIClient",
    "dev": "npm run build --watch",
    "lint": "eslint ./src/*.ts",
    "prepare": "npm run build"
  },
  "dependencies": {
    "globby": "^14.1.0",
    "isomorphic-ws": "^5.0.0",
    "pino": "8.21.0",
    "uuid": "9.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cz-commitlint": "^19.8.1",
    "@types/debug": "^4.1.12",
    "@types/node": "18.19.123",
    "@types/uuid": "9.0.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "commitizen": "^4.3.1",
    "commitlint": "^19.8.1",
    "eslint": "^8.44.0",
    "husky": "^9.1.7",
    "inquirer": "^9.3.7",
    "lint-staged": "^16.1.5",
    "semantic-release": "^24.2.7",
    "tsup": "7.2.0",
    "typescript": "~5.9"
  },
  "directories": {
    "example": "examples"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184"
}
